<%= form_with model: book do |form| %>
  <div>
    <%= form.label :book_name %><br>
    <%= form.text_field :book_name %>

  </div>
<div>
    <%= form.label :price %><br>
    <%= form.text_field :price %>
</div>
<div>
    <%= form.label :author_name %><br>
    <%= form.text_field :author_name %>
</div>

  <div>
    <%= form.submit %>
  </div>
<% end %>
